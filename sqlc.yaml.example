version: "2"
sql:
  - schema: "db/schema.sql"
    queries: "db/queries/"
    engine: "mysql"  # æˆ– "postgresql"
    gen:
      go:
        package: "db"
        out: "db/"
        sql_package: "database/sql"
        emit_json_tags: true
        emit_prepared_queries: false
        emit_interface: true
        emit_exact_table_names: false
        emit_empty_slices: true
